#include<stdio.h>
int main() {
 
	int tc, T;
	scanf("%d", &T);
	for (tc = 0; tc < T; tc++) {
		long long int m, n;
		scanf("%lld%lld", &n, &m);
		if (n == 1 && m == 1) {
			printf("No\n");
		}
		else if ((n == 1 && m == 2) || (n == 2 && m == 1)) {
			printf("Yes\n");
		}
 
		else if (n==1 || m==1) {
			printf("No\n");
		}
 
		else if (n % 2 == 0 || m % 2 == 0) {
			printf("Yes\n");
		}
		else {
			printf("No\n");
		}
	}
 
	return 0;
}  
