#include<stdio.h>
 
int main()
{
    char arr[210];
    int i,j,cost[110]={0},t,k;
    scanf("%d",&t);
    for(k=0;k<t;k++)
    {
    scanf("%s",arr);
    for(i=0;arr[i]!='\0';i++)
    {
                             if(arr[i]=='0');
                             else
                             {
                              for(j=i+1;arr[j]!='\0';j++)
                              {
                                                         if(arr[i]==arr[j])
                                                        {
                                                                      arr[i]='0';
                                                                      arr[j]='0';
                                                                      break;
                                                                      }
                                                            }
                              cost[k]++;
                                                            }
                                                            }
  }
  for(i=0;i<t;i++)
  {
    printf("\n%d",cost[i]);
}
    return 0;
}
 
